{
	"name": "flex_buffers",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"extensions": [
		"ms-azuretools.vscode-docker",
		"ms-vscode.cpptools"
	],
	"workspaceMount": "",
	"workspaceFolder": "/workspaces/flex_buffers",
	"runArgs": [
		"--volume=${localWorkspaceFolder}/../../:/workspaces/flex_buffers",
		"--volume=${env:HOME}/.ssh:/root/.ssh",
		"--cap-add=SYS_PTRACE"
	]
}